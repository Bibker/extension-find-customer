{
    "manifest_version": 2,
    "name": "Customer Details Finder (VCTS)",
    "description": "Find Customer Details ",
    "version": "1.0.0",
    "icons": { "128": "icon.png" },
    "browser_action": {
        "default_icon": "icon.png"

    },
    "content_scripts": [{
        "js": ["scripts/jquery.js", "scripts/content.js"],
        "matches": ["<all_urls>"]

    }],
    "background": {
        "scripts": ["scripts/jquery.js", "scripts/background.js"]
    }

}